EAAE,EAAC,WAAR;AAAoB,MAAA,SAAS,EAAC,yBAA9B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAM,MAAA,QAAQ,EAAE,KAAKf,cAArB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAK,MAAA,SAAS,EAAC,YAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAO,MAAA,OAAO,EAAC,WAAf;AAA2B,MAAA,SAAS,EAAC,0BAArC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAgE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,oDAAhE,EACI;AAAQ,MAAA,IAAI,EAAC,WAAb;AAAyB,MAAA,SAAS,EAAC,cAAnC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qCADJ,EAEI;AAAQ,MAAA,KAAK,EAAE,KAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gBAFJ,EAGI;AAAQ,MAAA,KAAK,EAAE,KAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gBAHJ,EAII;AAAQ,MAAA,KAAK,EAAE,KAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gBAJJ,EAKI;AAAQ,MAAA,KAAK,EAAE,MAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iBALJ,EAMI;AAAQ,MAAA,KAAK,EAAE,MAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iBANJ,EAOI;AAAQ,MAAA,KAAK,EAAE,MAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iBAPJ,EAQI;AAAQ,MAAA,KAAK,EAAE,MAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iBARJ,EASI;AAAQ,MAAA,KAAK,EAAE,OAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBATJ,EAUI;AAAQ,MAAA,KAAK,EAAE,OAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAVJ,CADJ,CADJ,EAeI;AAAQ,MAAA,EAAE,EAAC,WAAX;AAAuB,MAAA,SAAS,EAAC,0BAAjC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAfJ,CADJ,CADJ,CANJ,CA/BJ,EA2DI;AAAK,MAAA,SAAS,EAAC,eAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uMA3DJ,CAZJ,EA2EI;AAAK,MAAA,SAAS,EAAC,UAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MA3EJ,CADJ,CADJ;AAiFH;;AAlL6B;;AAqLlC,MAAMmC,eAAe,GAAI7B,KAAD,KAAY;AAChC8B,EAAAA,WAAW,EAAE9B,KAAK,CAAC8B;AADa,CAAZ,CAAxB;;AAIA,MAAMC,kBAAkB,GAAG,CAACC,QAAD,EAAW5C,KAAX,KAAqB;AAC5C,SAAO;AACHmC,IAAAA,aAAa,EAAE,CAACE,IAAD,EAAOE,QAAP,KAAoB;AAC/BK,MAAAA,QAAQ,CAAChD,WAAW,CAACiD,oBAAZ,CAAiCR,IAAjC,EAAuCE,QAAvC,CAAD,CAAR;AACH;AAHE,GAAP;AAKH,CAND;;AAQA,eAAe7C,OAAO,CAAC+C,eAAD,EAAkBE,kBAAlB,CAAP,CAA6C7C,SAA7C,CAAf","sourcesContent":["import React, { Component } from 'react'\r\nimport { connect } from 'react-redux'\r\nimport \"../assets/css/paytogame.css\"\r\nimport PayBreadcrumb from './PayBreadcrumb'\r\nimport gameActions from \"../actions/games.actions\"\r\nimport Validate from \"../utils/Validate\";\r\n\r\nclass Paytogame extends Component {\r\n\r\n    constructor(props){\r\n        super(props)\r\n        this.state = {\r\n            sltServer: '',\r\n            sltCardType: '',\r\n            txtSerie: '',\r\n            txtCardPin: '',\r\n            sltAmount: '',\r\n            errors: []\r\n        }\r\n    }\r\n\r\n    componentDidMount(){\r\n        let {match} = this.props\r\n        let userInfo = localStorage.getItem('user') ? JSON.parse(localStorage.getItem('user'))  : {};\r\n        this.props.getGameDetail(match.params.slug, userInfo.data.username)\r\n    }\r\n\r\n    handlePayByCard = (e) =>{\r\n        e.preventDefault();\r\n        if(this.validatePayCard()){\r\n            alert(\"submitted\")\r\n        }\r\n    }\r\n\r\n    handlePayByATM = (e) => {\r\n        e.preventDefault();\r\n        if(this.validatePayAtm()){\r\n            alert(\"submitted\")\r\n        }\r\n    }\r\n\r\n    validatePayCard = () => {\r\n        const { sltServer, sltCardType, txtSerie, txtCardPin } = this.state;\r\n        let errors = {\r\n            sltServer: [],\r\n            sltCardType: [],\r\n            txtSerie: [],\r\n            txtCardPin: [],\r\n            sltAmount: []\r\n        };\r\n        let isValid = true;\r\n        if (Validate.isRequired( sltServer )) {\r\n            errors.sltServer.push('Vui lòng chọn server');\r\n            isValid = false;\r\n        }\r\n        if (Validate.isRequired( sltCardType )) {\r\n            errors.sltCardType.push('Vui lòng chọn loại thẻ');\r\n            isValid = false;\r\n        }\r\n        if (Validate.isRequired( txtSerie )) {\r\n            errors.txtSerie.push('Loại thẻ bắt buộc nhập');\r\n            isValid = false;\r\n        }\r\n        if (Validate.isRequired( txtCardPin )) {\r\n            errors.txtCardPin.push('Mã thẻ bắt buộc nhập');\r\n            isValid = false;\r\n        }\r\n        this.setState({\r\n            errors: errors\r\n        })\r\n        console.log(errors)\r\n        return isValid\r\n    }\r\n\r\n    validatePayAtm = () => {\r\n        const { sltAmount } = this.state;\r\n        let errors = {\r\n            sltAmount: []\r\n        };\r\n        let isValid = true;\r\n        if (Validate.isRequired( sltServer )) {\r\n            errors.sltServer.push('Vui lòng chọn server');\r\n            isValid = false;\r\n        }\r\n        if (Validate.isRequired( sltAmount )) {\r\n            errors.sltAmount.push('Vui lòng chọn mệnh giá');\r\n            isValid = false;\r\n        }\r\n        this.setState({\r\n            errors: errors\r\n        })\r\n        return isValid\r\n    }\r\n\r\n    handleChange = (event) =>{\r\n        const target = event.target;\r\n        const value = target.type === 'checkbox' ? target.checked : target.value;\r\n        const name = target.name;\r\n        this.setState({\r\n            [name]: value\r\n        });\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div className=\"container paytogame_container\">\r\n                <div className=\"row box\">\r\n                    <PayBreadcrumb></PayBreadcrumb>\r\n                    <h2>Thế Giới Cá</h2>\r\n                    <div className=\"hd-Form\"> Bước 1: Chọn thông tin nhân vật (*)</div>\r\n                    <label htmlFor=\"sltServer\" className=\"col-sm-12 controll-label\" id=\"list_server\"> Chọn server:</label>\r\n                    <select name=\"sltServer\" onChange={this.handleChange} className=\"form-control \" id=\"server_list\">\r\n                        <option value>Chọn server</option>\r\n                        <option value=\"TGC\">TGC</option>\r\n                    </select>\r\n                    <div id=\"appentHtml\" />\r\n                    <div className=\"clearfix\" /> <input type=\"hidden\" name=\"id_user\" id=\"id_user\" />\r\n                    <div className=\"hd-Form\"> Bước 2: Chọn phương thức thanh toán (*)</div>\r\n                    <div className=\"panel-group paytogame\" id=\"accordion\">\r\n                        <div className=\"panel panel-default\">\r\n                            <div className=\"panel-heading\">\r\n                                <h4 className=\"panel-title\">\r\n                                    <a data-toggle=\"collapse\" data-parent=\"#accordion\" href=\"#collapse1\">Nạp thẻ cào</a>\r\n                                </h4>\r\n                            </div>\r\n                            <div id=\"collapse1\" className=\"panel-collapse collapse in\">\r\n                                <form onSubmit={this.handlePayByCard}>\r\n                                    <div className=\"panel-body\">\r\n                                        <label htmlFor=\"sltCardType\" className=\"col-sm-12 controll-label\">\r\n                                            <span>Loại thẻ</span>\r\n                                            <select name=\"sltCardType\" onChange={this.handleChange} className=\"form-control\">\r\n                                                <option value=\"\">Chọn loại thẻ</option>\r\n                                                <option value=\"HPC\">HPCode</option>\r\n                                                <option value=\"GATE\">GATE</option>\r\n                                            </select>\r\n                                        </label>\r\n                                        <label htmlFor=\"txtSerie\" className=\"col-sm-12 controll-label\">\r\n                                            <span>Số serie</span>\r\n                                            <input type=\"text\" name=\"txtSerie\" className=\"form-control\" onChange={this.handleChange} />\r\n                                        </label>\r\n                                        <label htmlFor=\"txtCardPin\" className=\"col-sm-12 controll-label\">\r\n                                            <span>Mã thẻ</span>\r\n                                            <input type=\"text\" name=\"txtCardPin\" className=\"form-control\" onChange={this.handleChange} />\r\n                                        </label>\r\n                                        <button id=\"submitCard\" className=\"col-sm-3 btn btn-primary\">Thanh toán</button>\r\n                                    </div>\r\n                                </form>\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"panel panel-default\">\r\n                            <div className=\"panel-heading\">\r\n                                <h4 className=\"panel-title\">\r\n                                    <a data-toggle=\"collapse\" data-parent=\"#accordion\" href=\"#collapse2\">Nạp thẻ ATM/iBanking</a>\r\n                                </h4>\r\n                            </div>\r\n                            <div id=\"collapse2\" className=\"panel-collapse collapse\">\r\n                                <form onSubmit={this.handlePayByATM}>\r\n                                    <div className=\"panel-body\">\r\n                                        <label htmlFor=\"sltAmount\" className=\"col-sm-12 controll-label\"><span>Số tiền thanh toán (VNĐ)</span>\r\n                                            <select name=\"sltAmount\" className=\"form-control\">\r\n                                                <option>Chọn số tiền</option>\r\n                                                <option value={10000}>10,000</option>\r\n                                                <option value={20000}>20,000</option>\r\n                                                <option value={50000}>50,000</option>\r\n                                                <option value={100000}>100,000</option>\r\n                                                <option value={200000}>200,000</option>\r\n                                                <option value={300000}>300,000</option>\r\n                                                <option value={500000}>500,000</option>\r\n                                                <option value={1000000}>1,000,000</option>\r\n                                                <option value={2000000}>2,000,000</option>\r\n                                            </select>\r\n                                        </label>\r\n                                        <button id=\"submitAtm\" className=\"col-sm-3 btn btn-primary\">Thanh toán</button>\r\n                                    </div>\r\n                                </form>\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"message-alert\">\r\n                            NẠP HPCODE = 100% ; GATE = 100% ; VCOIN = 90% ; ATM = 100% (Thẻ HPCode có bán ở các cửa hàng của Payoo &amp; bán online tại Website Napthegame.net )\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"clearfix\" />\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nconst mapStateToProps = (state) => ({\r\n    gameReducer: state.gameReducer\r\n})\r\n\r\nconst mapDispatchToProps = (dispatch, props) => {\r\n    return {\r\n        getGameDetail: (slug, username) => {\r\n            dispatch(gameActions.getGameDetailRequest(slug, username))\r\n        }\r\n    }\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Paytogame)"]},"metadata":{},"sourceType":"module"}